          <BIL A20="HDQ" A22="80K2" A70="WPIPA" AA0="-AZ" AD0="6751F8" AE0="AA" C00="-2481571921086059611" C20="MANUALCONSTRUCTEDXML" S0R="LBTY" Q02="3470" Q03="925" />
          <PRO C10="199" C45="USD" D07="2015-07-28" D54="0983" PBK="T" N08="B" Q0P="1" P0J="T" S14="MANUALLY CONSTRUCTED" SEZ="T" SEY="T" PPC="T" S15="CRSAGT" />
          <DIG S01="" />
          <DIG S01="" />
          <PXI B70="ADT" Q0U="1" />
          <SGI Q0C="1">
            <FLI Q0C="1" N03="A" B00="NK" B01="NK" A01="MCO" A02="DFW" B30="Y" D01="2015-08-19" Q0B="405" D02="2015-08-19" D00="2015-07-28" D31="0755" D32="0857" D30="0983" BB2="GK" BB0="OK" B40="319" />
