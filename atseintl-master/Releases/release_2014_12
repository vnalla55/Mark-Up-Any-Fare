          <MSG N06="X" Q0K="10" S18="FARE BASIS CODE: J3DEGOW" />
          <MSG N06="X" Q0K="11" S18="FARE CALC FARE AMOUNT: 3575.30 NUC" />
          <MSG N06="X" Q0K="12" S18="HIP INCLUDED: NO" />
