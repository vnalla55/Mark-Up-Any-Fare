                <IDS A01="SYD" A02="MEL" A70="SS" B00="VA" B30="N" D01="2015-08-01" D02="2015-08-01" D31="0600" D32="0735" N0V="J" P2X="ET" Q0B="800" Q0U="01"/>
                <IDS A01="MEL" A02="SYD" A70="SS" B00="VA" B30="E" D01="2015-08-10" D02="2015-08-10" D31="0600" D32="0725" N0V="J" P2X="ET" Q0B="803" Q0U="01"/>
