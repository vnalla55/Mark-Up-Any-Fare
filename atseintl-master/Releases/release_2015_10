            <BIL A20="HDQ" A22="SYD" A70="WPQ/*" AA0="GS0" AD0="457ECB" AE0="VA" C01="445592692004090374" C20="INTLWPI1" S0R="PPSS"/>
            <PRO AE0="VA" B05="VA" C45="AUD" D07="2015-07-02" D54="1144" N08="B" P0J="T" PBK="T" PBZ="F" PPC="T" Q0P="1" Q16="03" RTW="F" S14="WPQ/*196" S15="CRSAGT" SEY="T"/>
