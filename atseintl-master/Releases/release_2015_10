        <AncillaryPricingRequest Version="1.0.2" xmlns="_http://www.atse.sabre.com/AncillaryPricing/Request">
          <AGI A10="SYD" N0G="5" B00="1S" AE1="SYDAI" AE2="0239232" />
          <BIL AD0="60EBEF" AE0="VA" />
          <PRO PS1="T" C10="875" />
          <DIG Q0A="30" S01="SG99" />
          <ITN Q00="1">
            <IRO>
              <PXI B70="ADT" />
            </IRO>
            <SGI Q00="1">
              <FLI A01="MEL" A02="WLG" D01="2015-09-11" D31="1800" D02="2015-09-11" D32="2340" B00="DL" B01="NZ" B30="U" Q0B="7520" S95="320" />
            </SGI>
