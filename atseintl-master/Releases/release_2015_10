            <SGI Q0C="01" Q6D="01" B50="NSAVER">
                <FLI A01="SYD" A02="MEL" B00="VA" B01="VA" B30="N" BB0="OK" BB2="SS" D00="2015-07-02" D01="2015-08-01" D02="2015-08-01" D30="1144" D31="0360" D32="0455" N03="A" Q0B="800" Q0C="01"/>
            </SGI>
