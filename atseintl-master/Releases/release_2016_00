         <BIL A20="HDQ" A22="80K2" A70="WPRW " AA0="-3H" AD0="0A7566" AE0="AA" C00="5896734361027337850" C20="INTLHIST" S0R="LBTY" Q02="0" Q03="0" />
         <PRO C45="USD" B="T" D07="2015-09-18" D54="0001" PBK="T" N08="B" Q0P="1" P0J="T" PBZ="T" RTW="T" S13="FDANFA" S14="WPRW B18SEP15 C-AY Q/*196" SEZ="T" SEY="T" PPC="T" S15="CRSAGT" C10="" />
