          <DIG S01="" />
          <PXI B70="ADT" Q0U="01" />
          <EXC>
            <PRO B05="AM" C6P="USD" C45="MXN" C6Y="NUC" C5E="01680.21" D93="2012-07-26" D07="2012-12-18" D54="0722" AE0="AM" PBZ="T" Q0P="1" Q6B="01.000000000" C5A="1680.00" Q16="03" P0J="T" S13="OOXVKX" S14="WF " P69="T" />
            <SGI Q0C="01" B50="ONE03CQ1" A01="ELP" A02="DFW" Q6D="01" PCI="T" C48="MXN">
