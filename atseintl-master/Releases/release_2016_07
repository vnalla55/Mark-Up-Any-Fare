          <EXC>
            <AGI A10="HEL" A20="DR93" A21="HDQ" A80="DR93" A90="GHZ" AB0="19209875" AB1="19209875" AE0="AA" B00="1S" C40="EUR" N0G="*" />
            <PRO AE0="AA" B05="AA" C45="EUR" C5A="2470.00" C6P="USD" C6Y="NUC" Q0P="1" Q16="03" />
            <PXI B70="ADT" Q0U="01" />
            <SGI A01="DFW" A02="LHR" B09="AA" B50="KHX0S1C3" C50="1235.00" PCI="F" Q0C="1" Q6D="1" S37="ATP" S89="1" S90="J575">
