<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="CommonTypes.xsd"/>
  <xs:include schemaLocation="CommonElements.xsd"/>
  <xs:element name="ShoppingResponse">
    <xs:annotation>
      <xs:documentation>
	  
        ShoppingResponse

        ADP   Alternate DatePair that reach the cutoff threshold
        ADS   Alternate DatePairs with sold out condition
        OND   O and D for all thru fare Market sending to MIP
        ITN   Itinerary Return
        DFL   BFF and FF response of Date and  Flight list
        DIA   Diagnostic Response
        INF   Shopping Information
        Q0S   number of solutions return
        Q0F   number of solutions priced
        Q1S   number of full soldout solutions in IBF
        S79   Server Hostname
        PHL   Hurry Logic indicator (if true, processing had to hurry)
        C6W   Alternate Date Cutoff Nuc Threshold
        STK   Server Identify token (ISELL use it to shopping server identify)
        BFO   Branded Fares Sold Out by ASE
        S07   Segment Fee Message
        NLF   SOL performed switch to THRU only mode or skipped local FM creation
		
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ADP" type="ADPType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="OND" type="ONDType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="ITN" type="ITNType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="DFL" type="DFLType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="DIA" type="DIAType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="INF" type="INFType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="BFO" type="BFOType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="LTC" type="LTCType" minOccurs="0"/>
        <xs:element name="PTI" type="PTIType" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Q0S" type="xs:short" use="optional"/>
      <xs:attribute name="Q0F" type="xs:short" use="optional"/>
      <xs:attribute name="Q1S" type="xs:short" use="optional"/>
      <xs:attribute name="S79" type="xs:string" use="optional"/>
      <xs:attribute name="C6W" type="MonetaryAmountType" use="optional"/>
      <xs:attribute name="STK" type="xs:string" use="optional"/>
      <xs:attribute name="S07" type="xs:string" use="optional"/>
      <xs:attribute name="BSL" type="xs:string" use="optional"/>
      <xs:attribute name="PHL" type="BoolTFType" use="optional" default="F"/>
      <xs:attribute name="NLF" type="BoolTFType" use="optional" default="F"/>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ADPType">
    <xs:annotation>
      <xs:documentation>

        ADPType  Alternate DatePair that reaches the threshold
		
        D01  Outbound DatePair
        D02  InBound DatePair
		
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="D01" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="D02" type="DateYYYY-MM-DDType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="ADSType">
    <xs:annotation>
      <xs:documentation>

        ADSType   Alternate DatePairs with sold out condition
		
        D01   Outbound Date
        D02   InBound Date
		
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="D01" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="D02" type="DateYYYY-MM-DDType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="ONDType">
    <xs:annotation>
      <xs:documentation>
	  
        ONDType   O and D for all thru fare Market sending to MIP
		
        A01 Boarding City
        A02 Arrival City
        D01 Flight Departure Date
		
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="A01" type="AirportCodeType" use="required"/>
    <xs:attribute name="A02" type="AirportCodeType" use="required"/>
    <xs:attribute name="D01" type="DateYYYY-MM-DDType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="ITNType">
    <xs:annotation>
      <xs:documentation>
	  
        ITNType Itinerary Return
		
        SID  SIDType return SOPID
        IFL  List of flight segments (element used on subitinerary ITT level).
        SEG  SEGType travelSeg
        PSG  PSGType Passeger Fare Amount
        TOT  TOTType Grand Total
        VCL  VCLType Validating Carrier List Information
        MMS  Node for Min Max Stay Indicator
        B05  validating carrier
        C65  Total Tax return from IS request
        D00  Last day to ticket
        D14  Advanced Purchase Date
        D16  Purchase By Date
        P0N  Change class must rebook
        P0J  eticketable Itinerary (this itinerary can eeticket)
        PBC  Simultaneous Reservation
        P35  Divide Party Indicator
        P95  is international
        PTF  Are all fare markets of the fare path a thru fare markets?
        PXP  JCB exist
        Q4Q  Carrier Group
        Q6Q  Itineraris grouping key. It's value will be used by InteliSell to group itineraries from different IS responses.
             Value could be either: 
               carrier code (preceded by "_" in case of interline solutions), 
               outboundDate + inboundDate (if second leg exists),
               carrier code + outbound date + inbound date (if second leg exists).
        Q5Q  Family Number that itin belongs to
        N1U  Private Fare Type Indicator
             *  fares filed with a corporate id or account code
             /  fare marked up via cat 35
             X ticketing ineligible cat 35 fares
             @ all other private fares
        TYP  Pricing type indicator:
             M - MIP
             B - BIP
             T - TAX
        UFL - Totally Unflown Itinerary Price Information
        MSG - Message Info - error message returned only for exchange MIP response for current itin
        SFG - Service Fee Group Information
        N27   Reissue vs. Exchange Indicator
        D95 - ROE and BSR Date
        T95 - ROE and BSR Time	
        ITT - SNAP itin info
        VTA - Interline Ticketing Agreement Failed (T - itin carriers do not have interline ticketing agreement)
        BRI - Branded itins
        GRI - Itins with fare group/brand info
        Q14 - Leg Id
        NUM - itin num
        ANY - Attribute returned in MIP-B transactions only. Set to true if any brand has been found for a given itinerary.
        SFY - Segment Fee Message resent
        PVT - Private indicator (T - itin contains at least one private fare)
        FBR - Fare By Rule indicator (T - itin contains at least one Fare by Rule fare)
        STI - Side Trip fare market indicator (T - itin contains at least one FM with side trip travel segment)
        CPL - List of GOV carriers per leg separated by "|"
        TFO - SOL looked in local fares but selected THRU one, hint for MIP
        FML - Family indicator. If FML="M", this means it is a mother itin. If FML is a number, this means this solution is a child of mother itin NUM="number"
        PY5 - Spanish Discount indicator with values of "A", "B", "C" where
              "A" indicates Spanish Large Family discount only
              "B" indicates Spanish Large Family discount + Spanish Islander discount
              "C" indicates Spanish Islander discount only

      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SID" type="SIDType" maxOccurs="unbounded"/>
      <xs:element name="IFL" type="IFLType" minOccurs="0"/>
      <xs:element name="SEG" type="SEGType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="PSG" type="PSGType" maxOccurs="unbounded"/>
      <xs:element name="TOT" type="TOTType"/>
      <xs:element name="VCL" type="VCLType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="MMS" type="MMSType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="UFL" type="UFLType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="MSG" type="MSGType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="SFI" type="SFIType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ITT" type="ITNType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="BRI" type="BRIType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="GRI" type="GRIType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="B05" type="AirlineCodeType" use="optional"/>
    <xs:attribute name="C65" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="D00" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="D14" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="D16" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="P0N" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P0J" type="BoolTFType" use="optional" default="T"/>
    <xs:attribute name="PBC" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P35" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P95" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="PTF" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="PXP" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="Q4Q" type="xs:short" use="optional"/>
    <xs:attribute name="Q6Q" type="xs:string" use="optional"/>
    <xs:attribute name="Q5Q" type="xs:short" use="optional"/>
    <xs:attribute name="N1U" type="AnyCharType" use="optional"/>
    <xs:attribute name="TYP" type="AnyCharType" use="optional"/>
    <xs:attribute name="NUM" type="xs:short" use="optional"/>
    <xs:attribute name="ANY" type="BoolTFType" use="optional"/>
    <xs:attribute name="SFY" type="BoolTFType" use="optional"/>
    <xs:attribute name="N27" use="optional">
      <xs:simpleType>
        <xs:restriction base="AlphaNumCharType">
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="D95" type="DateYYYY-MM-DDType" use="optional"/>
    <xs:attribute name="T95" type="TimeHHMMColonType" use="optional"/>
    <xs:attribute name="VTA" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="PVT" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="Q14" type="xs:short" use="optional"/>
    <xs:attribute name="CPL" type="xs:string" use="optional"/>
    <xs:attribute name="TFO" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="FML" type="xs:string" use="optional"/>
  </xs:complexType>
  <xs:complexType name="SIDType">
    <xs:annotation>
      <xs:documentation>

        SID  SIDType return SOP ID assosicate to the request
		
        Q14  Leg ID
        Q15  SOP ID
        BKK  Booking Code and cabin requested
		
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BKK" type="BKKType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="Q14" type="xs:short" use="required"/>
    <xs:attribute name="Q15" type="xs:short" use="required"/>
  </xs:complexType>
  <xs:complexType name="BKKType">
    <xs:annotation>
      <xs:documentation>
	  
        BKKType   Booking code and cabin return from IS

        B30    IS Suggested Booking code
        Q13    Cabin Number (IS suggested for MIP request)
        B31    Booking Code Type (P, F, J, C, S, Y)
        Q3S    Number of Seat Remaining
		
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="B30" type="BookingCodeType" use="optional"/>
    <xs:attribute name="Q13" type="xs:short" use="optional"/>
    <xs:attribute name="B31" type="AnyCharType" use="optional"/>
    <xs:attribute name="Q3S" type="xs:short" use="optional"/>
  </xs:complexType>
  <xs:complexType name="SEGType">
    <xs:annotation>
      <xs:documentation>

        SEG  SEGType Travel segment witin Itinerary for MIP response
		
        BKC     BookingCode
        P2I     Extra mileage allowance
        P2L     Mileage reduction point
        P2M     Stopover segment
        P2H     Connection segment
        S09     segUnchargableSurface
        S10     SegPureSurface
        SC0     Program Code
        SC1     Program System Code
        SC2     Program Name
        SB2     Brand Code
        SB3     Brand Name

      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BKC" type="BKCType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="P2I" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P2L" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P2M" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="P2H" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="S09" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="S10" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="SC0" type="xs:string" use="optional"/>
    <xs:attribute name="SC1" type="xs:string" use="optional"/>
    <xs:attribute name="SC2" type="xs:string" use="optional"/>
    <xs:attribute name="SB2" type="xs:string" use="optional"/>
    <xs:attribute name="SB3" type="xs:string" use="optional"/>

  </xs:complexType>
  <xs:complexType name="PSGType">
    <xs:annotation>
      <xs:documentation>
	  
        PSG     PSGType Passenger Fare Amount

        FDC     FDCType Fare Details Component Type
        TAD     TADType Tax
        CCD     Currency conversion info
        MSG     Message Info
        SFD     Selling Fare Data
        EPD     Effective Dynamic Price Deviation Amount in equivalent currency
        PUP     Plus Up Information ex; LCM
        LEG     Leg taxes
        Q0U     Number of Passenger of this Type
        Q79     Total number of passengers
        B70     Passenger type
        C40     Base Fare Currency Code (origination Currency)
        C43     Consturction Currency Code
        C44     Local currency min currency co
        C46     Equivalent Currency Code / sell currency (Tax Currency Code)
        C54     Exchange Rate One
        C63     Stopover charges / unspecified
        C66     Total Fare + Tax per Passenger (in equivalent currency code)
        C65     Total Tax (in C46 equivalent currency code)
        C82     Total T-type OB Fee per passenger(in C46 equivalent currency code)
        C5A     Base Fare Amount
        C5B     Commission amount
        C5C     Local currency min amount
        C5D     Commission percentage
        C5E     Construction total amount
        P27     Non refundable indicator
        Q05     Number Decimal Places Exchange Rate One
        Q0X     Stopover count
        S07     Fare Line Data
        N1U     Private Fare Type Indicator
		        *   fares filed with a corporate id or account code
                /   fare marked up via cat 35
                X   ticketing ineligible cat 35 fares
                @   all other private fares
        S30     Bulk indicator
        OBG     Fees (OB)
        REX     Reissue Information
        SF1     OC Fees Summary Amount
        C5L     Mileage Total
        NRA     Non-refundable Base Fare Amount (C40 is a currency)

        C61     Cat 35 Commission Percentage
        C60     Cat 35 Commission Amount
        C57     Cat 35 Markup Amount (in equivalent amount currency)
        C58     Commission Amount (in equivalent amount currency)  
        C5H     Validating Carrier Commission Indicator
        CT0     CommissionSource [value C for Cat 35, A for AMC, M for Manual]
        VCC     Validating Carrier Commission Information
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FDC" type="FDCType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="TAD" type="TADType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="CCD" type="CCDType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="MSG" type="MSGType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="SFD" type="SFDType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="PUP" type="PUPType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="OBG" type="OBGType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="REX" type="REXType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="LEG" type="LEGType" minOccurs="0" maxOccurs="2"/>
      <xs:element name="BDI" type="BDIType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="PEN" type="PENType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="VCC" type="VCCType" minOccurs="0" maxOccurs="23" use="optional"/>
    </xs:sequence>
    <xs:attribute name="Q0U" type="xs:short" use="required"/>
    <xs:attribute name="B70" type="PassengerCodeType" use="required"/>
    <xs:attribute name="Q79" type="xs:short" use="required"/>
    <xs:attribute name="C40" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C43" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C44" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C46" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C54" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C63" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C66" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C65" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C82" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5A" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5B" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5C" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5D" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5E" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="EPD" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="P27" type="BoolTFType" use="optional" default="F"/>
    <xs:attribute name="Q05" type="xs:short" use="optional"/>
    <xs:attribute name="Q0X" type="xs:short" use="optional"/>
    <xs:attribute name="S07" type="xs:string" use="optional"/>
    <xs:attribute name="N1U" type="AnyCharType" use="optional"/>
    <xs:attribute name="S30" type="xs:string" use="optional"/>
    <xs:attribute name="SF1" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5L" type="xs:integer" use="optional"/>
    <xs:attribute name="NRA" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C61" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C60" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C57" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C58" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5H" type="BoolTFType" use="optional" default="N"/>    
    <xs:attribute name="CT0" type="AnyCharType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="VCCType">
    <xs:annotation>
      <xs:documentation>
        Validating Carrier Commission Information
        B00 - Validating Carrier
        C58 - Commission Amount (in equivalent amount currency)
        C60 - Total Commission amount including Mark-Up
        C61 - Commission Percent
        FCB - FC Breakdown, 1 to many   [per Fare Component ]
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FCB" type="FCBType" minOccurs="0" maxOccurs="24"/>
    </xs:sequence>
    <xs:attribute name="B00" type="AirlineCodeType" use="required"/>  
    <xs:attribute name="C58" type="MonetaryAmountType" use="required"/>
    <xs:attribute name="C60" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C61" type="MonetaryAmountType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="FCBType">
    <xs:annotation>
      <xs:documentation>
        FCB     FCBType FC Breakdown, 1 to many   [per Fare Component ] (VCC)

        Q6D - Fare Component Number
        C58 - AMC per Fare Component
        C3R - Commission Rule ID
        C3P - Commission Program ID
        C3C - Commission Contract ID
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="Q6D" type="xs:unsignedShort" use="optional"/>
    <xs:attribute name="C58" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C3R" type="xs:unsignedLong" use="optional"/>
    <xs:attribute name="C3P" type="xs:unsignedLong" use="optional"/>
    <xs:attribute name="C3C" type="xs:unsignedShort" use="optional"/>
  </xs:complexType>
  <xs:complexType name="PENType">
    <xs:annotation>
        <xs:documentation>
                    PENType - Penalty information
                    CPB - Change Penalty Before Departure
                    CPA - Change Penalty After Departure
                    RPB - Refund Penalty Before Departure
                    RPA - Refund Penalty After Departure
        </xs:documentation>
    </xs:annotation>
    <xs:sequence>
        <xs:element name="CPB" type="PenaltyInfoType" minOccurs="0"/>
        <xs:element name="CPA" type="PenaltyInfoType" minOccurs="0"/>
        <xs:element name="RPB" type="PenaltyInfoType" minOccurs="0"/>
        <xs:element name="RPA" type="PenaltyInfoType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PenaltyInfoType">
    <xs:annotation>
      <xs:documentation>
                  PenaltyType - Penalty information
                  MDT - Missing Data Information
                  NON - Indicates if the change in a fare is non applicable (eg. for refund that means nonrefundability)
                  MPA - Penalty Amount
                  MPC - Penalty Currency
                  C16 - Indicates CAT16 information source
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MDT" type="MDTType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="NON" type="xs:boolean" use="optional" default="false"/>
    <xs:attribute name="MPA" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="MPC" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C16" type="xs:boolean" use="optional" default="false"/>
  </xs:complexType>
  <xs:complexType name="MDTType">
    <xs:annotation>
      <xs:documentation>
        B50 - Fare Basis Code
        Q20   Fare Component Id
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="B50" type="FareBasisCodeType" use="required"/>
    <xs:attribute name="Q20" type="xs:integer" use="required"/>
  </xs:complexType>
  <xs:complexType name="TOTType">
    <xs:annotation>
      <xs:documentation>
	  
        TOT     TOTType Grand Total

        C43     Consturction Currency Code
        C5E     Construction total amount
        C40     Base Fare Currency Code (Origination Currency Code)
        C5A     Base Fare Amount
        C45     Equivalent Currency Code
        C5F     Equivalent Amount
        C46     Tax Currency Code
        C65     Total Tax
        C56     Total all pax Types (Currency Code is C46 equivalent amount)
        EPD     Effective Dynamic Price Deviation Amount in equivalent currency
        C5H     ESV for calling Tax Service - base fare amount in selling currency
        C5I     ESV for calling Tax Service - base fare amount +surcharge in selling currency
        C4C     ESV for calling Tax Service - base selling currency
        SF1     OC Fees Summary Amount Total
        C5L     Mileage Total
		    C67     Base Fare Amount (doesn't include taxes and surcharges)
        C82     Total T-type OB Fee for all passengers(in C46 equivalent currency code)

      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="C43" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C5E" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C40" type="CurrencyCodeType" use="required"/>
    <xs:attribute name="C5A" type="MonetaryAmountType" use="required"/>
    <xs:attribute name="C45" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C5F" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C46" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C65" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C56" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="EPD" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C4C" type="CurrencyCodeType" use="optional"/>
    <xs:attribute name="C5H" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5I" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="SF1" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C5L" type="xs:integer" use="optional"/>
    <xs:attribute name="C67" type="MonetaryAmountType" use="optional"/>
    <xs:attribute name="C82" type="MonetaryAmountType" use="optional"/>
  </xs:complexType>
  <xs:complexType name="MMSType">
    <xs:annotation>
      <xs:documentation>

        MMS     MMSType Node for Min Stay Max Stay indicator

